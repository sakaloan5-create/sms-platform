{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "your-jwt-secret-change-this",
    "JWT_EXPIRES_IN": "24h",
    "FRONTEND_URL": "https://sakaloan5-create.github.io/sms-platform"
  }
}
